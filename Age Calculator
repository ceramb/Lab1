#!/usr/bin/env python3

def ageCalc():
    try:
        bdate=input('Enter date of birth: ')
        cyear=input('Enter current year: ')
        age= int(cyear) -int(bdate)
        return age
    except(ValueError):
        return('Please enter an int')

print (ageCalc())


helloWorld()


